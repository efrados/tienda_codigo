<%= turbo_frame_tag dom_id(query_product) do %>
  <%= form_with(model: [query_product, current_user.query_recommendations.find_by(query_product_id: query_product.id)],
          html: { method: :delete }) do |f| %>
    <%= f.button bicon('bi-hand-thumbs-down -fill', class: 'dot dot-rmv', style: 'color: blue'),
        class: 'btn-heart', 'data-bs-toggle': 'tooltip', 'data-bs-placement': 'right',
        title: 'Remove Favorite' %>
  <% end %>
<% end %>
